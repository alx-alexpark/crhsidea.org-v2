{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "framer-motion": "^6.5.1",
    "next": "^12.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.0-beta.10",
    "sass": "^1.54.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "git init && husky install",
    "lint": "next lint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.3",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "import/no-anonymous-default-export": [
        1,
        {
          "allowObject": true
        }
      ],
      "react/jsx-pascal-case": [
        2,
        {
          "allowNamespace": true
        }
      ],
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "prefer-const": [
        1,
        {
          "destructuring": "all"
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --quiet --fix --cache"
  }
}
